216 {\cf18 void} managerMenu(Manager **head, Vehicle **vehicle, Client **client, Manager *m, Graph **g, History **hist)\par
217 \{\par
218   {\cf18 int} opt;\par
219   Manager *current = *head;\par
220 \par
221   clearConsole();\par
222   printf({\cf22 "Welcome %s\\n"}, m->name);\par
223   system({\cf22 "cat ./Menus/managers/managersMenu.txt"});\par
224   fflush(stdout);\par
225   scanf({\cf22 "%d"}, &opt);\par
226 \par
227   {\cf19 switch} (opt)\par
228   \{\par
229   {\cf19 case} 1:\par
230     {\cf20 // Add vehicle}\par
231     clearConsole();\par
232     printf({\cf22 "Vehicle to add:\\n"});\par
233     vehicleReg(vehicle, g);\par
234     getchar();\par
235     enterToContinue();\par
236     {\cf19 break};\par
237   {\cf19 case} 2:\par
238     {\cf20 // Check history}\par
239     clearConsole();\par
240     fflush(stdout);\par
241     listHistory(hist);\par
242     enterToContinue();\par
243     {\cf19 break};\par
244   {\cf19 case} 3:\par
245     {\cf20 // Change vehicle}\par
246     clearConsole();\par
247     fflush(stdout);\par
248     listVehicles(vehicle);\par
249     printf({\cf22 "\\n"});\par
250     printf({\cf22 "Which vehicle do u use wish to change data from?\\nID: "});\par
251     {\cf18 int} vehicleID;\par
252     scanf({\cf22 "%d"}, &vehicleID);\par
253     clearConsole();\par
254     changeVehicleData(vehicle, g, vehicleID);\par
255     {\cf19 break};\par
256   {\cf19 case} 4:\par
257     {\cf20 // Change data}\par
258     clearConsole();\par
259     fflush(stdout);\par
260     current = changeManagerData(current);\par
261     {\cf19 break};\par
262   {\cf19 case} 5:\par
263     {\cf20 // List vehicles}\par
264     clearConsole();\par
265     fflush(stdout);\par
266     listVehicles(vehicle);\par
267     enterToContinue();\par
268     {\cf19 break};\par
269   {\cf19 case} 6:\par
270     {\cf20 // List clients}\par
271     clearConsole();\par
272     fflush(stdout);\par
273     listClients(client);\par
274     enterToContinue();\par
275     {\cf19 break};\par
276   {\cf19 case} 7:\par
277     {\cf20 // Remove clients}\par
278     clearConsole();\par
279     listClients(client);\par
280     printf({\cf22 "What is the ID of the client you wish to remove?\\n"});\par
281     {\cf18 int} remClient;\par
282     scanf({\cf22 "%d"}, &remClient);\par
283     removeClient(client, remClient);\par
284     enterToContinue();\par
285     {\cf19 break};\par
286   {\cf19 case} 8:\par
287     {\cf20 // Remove vehicle}\par
288     clearConsole();\par
289     listVehicles(vehicle);\par
290     printf({\cf22 "What is the ID of the client you wish to remove?\\n"});\par
291     {\cf18 int} remVehicle;\par
292     scanf({\cf22 "%d"}, &remVehicle);\par
293     removeVehicle(vehicle, remVehicle);\par
294     enterToContinue();\par
295     {\cf19 break};\par
296   {\cf19 case} 9:\par
297     {\cf20 // Sort vehicles by autonomy}\par
298     clearConsole();\par
299     fflush(stdout);\par
300     sortVehiclesByAutonomy(vehicle);\par
301     listVehicles(vehicle);\par
302     enterToContinue();\par
303     {\cf19 break};\par
304   {\cf19 case} 10:\par
305     {\cf20 // Delete account}\par
306     fflush(stdout);\par
307     removeManager(head, m->id);\par
308     printf({\cf22 "Account removed with success.\\n"});\par
309     enterToContinue();\par
310     clearConsole();\par
311     loginOrReg(client, head, vehicle, g, hist);\par
312     {\cf19 return};\par
313     {\cf19 break};\par
314   {\cf19 case} 11:\par
315     {\cf20 // Find shortest route with given vertexes;}\par
316     fflush(stdout);\par
317     clearConsole();\par
318 \par
319     {\cf18 float} dist[SIZE];\par
320     {\cf18 char} location[SIZE];\par
321     {\cf18 int} initial, {\cf17 final};\par
322     Graph *gAux = *g;\par
323 \par
324     printf({\cf22 "You want to go from what location to what location?\\n"});\par
325     listVertexes(g);\par
326     printf({\cf22 "Start: "});\par
327     scanf({\cf22 "%d"}, &initial);\par
328     printf({\cf22 "\\n"});\par
329 \par
330     printf({\cf22 "Finish: "});\par
331     scanf({\cf22 "%d"}, &{\cf17 final});\par
332 \par
333     convertIdToLocation(&gAux, initial, location);\par
334     {\cf18 char} initialLocation[SIZE];\par
335     strcpy(initialLocation, location);\par
336 \par
337     convertIdToLocation(&gAux, {\cf17 final}, location);\par
338     {\cf18 char} finalLocation[SIZE];\par
339     strcpy(finalLocation, location);\par
340 \par
341     dijkstra(gAux, initialLocation, finalLocation, dist, 0);\par
342     enterToContinue();\par
343     {\cf19 break};\par
344   {\cf19 case} 12:\par
345     clearConsole();\par
346     printf({\cf22 "All available vertexes:\\n"});\par
347     listVertexes(g);\par
348     enterToContinue();\par
349   {\cf19 case} 0:\par
350     clearConsole();\par
351     loginOrReg(client, head, vehicle, g, hist);\par
352     {\cf19 return};\par
353     {\cf19 break};\par
354   {\cf19 default}:\par
355     {\cf19 break};\par
356   \}\par
357   managerMenu(head, vehicle, client, m, g, hist);\par
358 \}\par
