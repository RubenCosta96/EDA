288 {\cf18 float} dijkstra(Graph *graph, {\cf18 char} *initial, {\cf18 char} *{\cf17 final}, {\cf18 float} *distances, {\cf18 int} {\cf18 id})\par
289 \{\par
290      Graph *aux = graph;\par
291      Graph *aux1 = graph;\par
292      {\cf18 int} visited[SIZE];\par
293      {\cf18 int} route[SIZE];\par
294      {\cf18 float} weight[SIZE];\par
295 \par
296      {\cf19 for} ({\cf18 int} i = 0; i < SIZE; i++)\par
297      \{\par
298           distances[i] = FLT_MAX;\par
299           visited[i] = 0;\par
300           route[i] = -1;\par
301           weight[i] = 0;\par
302      \}\par
303 \par
304      {\cf18 int} initialIndex = getIdByVertex(graph, initial);\par
305      distances[initialIndex] = 0;\par
306 \par
307      {\cf18 float} totalDistance = 0.0;\par
308      {\cf19 while} (1)\par
309      \{\par
310           Graph *minVertex = NULL;\par
311           {\cf18 float} minDistance = INFINITY;\par
312 \par
313           Graph *current = aux;\par
314           {\cf19 while} (current != NULL)\par
315           \{\par
316                {\cf19 if} (!visited[current->id] && distances[current->id] < minDistance)\par
317                \{\par
318                     minVertex = current;\par
319                     minDistance = distances[current->id];\par
320                \}\par
321                current = current->next;\par
322           \}\par
323 \par
324           {\cf19 if} (minVertex == NULL || strcmp(minVertex->vertex, {\cf17 final}) == 0)\par
325           \{\par
326                {\cf19 break};\par
327           \}\par
328 \par
329           visited[minVertex->id] = 1;\par
330 \par
331           Adjacent *adjacent = minVertex->adjacents;\par
332           {\cf19 while} (adjacent != NULL)\par
333           \{\par
334 \par
335                {\cf18 int} adjacentIndex = getIdByGeocode(aux1, adjacent->vertex);\par
336                {\cf19 if} (!visited[adjacentIndex] && distances[minVertex->id] != FLT_MAX &&\par
337                    distances[minVertex->id] + adjacent->weight < distances[adjacentIndex])\par
338                \{\par
339                     distances[adjacentIndex] = distances[minVertex->id] + adjacent->weight;\par
340                     route[adjacentIndex] = minVertex->id;\par
341                     weight[adjacentIndex] = adjacent->weight;\par
342                \}\par
343                adjacent = adjacent->next;\par
344           \}\par
345      \}\par
346      aux = aux->next;\par
347      {\cf18 int} finalIndex = getIdByVertex(graph, {\cf17 final});\par
348      {\cf19 return} printRoute(route, weight, finalIndex, {\cf18 id});\par
349 \}\par
